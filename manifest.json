{
  "name": "X logo to Twitter Logo",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Change X logo to the old Twitter logo",
  "icons": {
    "64": "size-64.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "permissions": ["favicon"]
}
